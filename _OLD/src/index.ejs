<!DOCTYPE html>
<html id="ng-app" ng-app="myApp" class="ng-app:myApp">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width">
    <title></title>
    <!--[if lte IE 8]>
        <script type="text/javascript" src="vendor/json3/lib/json3.min.js"></script>
        <script type="text/javascript" src="vendor/jquery/jquery.min.js"></script>    
    <![endif]-->

<%
    var styles = helpers.getIncludes('css');
    _.forEach(styles, function(style) {
%>      <link rel="stylesheet" href="<%=style%>"/>
<%
    });
%>
</head>
<body>

<div class="container-fluid">
    <div class="row-fluid">
        <div class="span12">
            <div data-navbar></div>
            <div data-notifications></div>
        </div>
    </div>
    <div class="row-fluid">
        <div ng-view></div>
    </div>
</div>
<%
    var scripts = helpers.getIncludes('js');
    _.forEach(scripts, function(script) {
%><script src="<%=script%>"></script>
<%
    });
%>
</body>
</html>